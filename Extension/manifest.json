{
  "manifest_version": 2,
  "name": "Github Trending",
  "description": "This extension shows trending Github projects",
  "version": "2.0",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "images/trending.png",
    "default_popup": "popup.html"
  },
  "author": "Oguzhan Demir",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "images/fire-16.png",
    "48": "images/fire-48.png",
    "128": "images/fire-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}